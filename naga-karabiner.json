{
    "title": "Example karabiner configuration file for a Razer Naga X and VSCode",
    "rules": [
        {
            "description": "New Window + Open...",
            "manipulators": [
                {
                    "type": "basic",
                    "conditions": [
                        {
                            "type": "device_if",
                            "identifiers": [
                                {
                                    "product_id": 150
                                }
                            ]
                        }
                    ],
                    "from": {
                        "key_code": "1"
                    },
                    "to": [
                        {
                            "key_code": "n",
                            "modifiers": [
                                "left_shift",
                                "left_command"
                            ]
                        }
                    ],
                    "to_delayed_action": {
                        "to_if_invoked": [
                            {
                                "key_code": "o",
                                "modifiers": [
                                    "left_command"
                                ]
                            }
                        ],
                        "to_if_canceled": [
                            {
                                "key_code": "o",
                                "modifiers": [
                                    "left_command"
                                ]
                            }
                        ]
                    },
                    "parameters": {
                        "basic.to_delayed_action_delay_milliseconds": 2000
                    }
                }
            ]
        },
        {
            "description": "Select All Occurrences",
            "manipulators": [
                {
                    "type": "basic",
                    "conditions": [
                        {
                            "type": "device_if",
                            "identifiers": [
                                {
                                    "product_id": 150
                                }
                            ]
                        }
                    ],
                    "from": {
                        "key_code": "2"
                    },
                    "to": [
                        {
                            "key_code": "l",
                            "modifiers": [
                                "left_shift",
                                "left_command"
                            ]
                        }
                    ]
                }
            ]
        },
        {
